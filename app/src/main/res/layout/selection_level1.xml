<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/colorLevel1">

    <android.support.design.widget.FloatingActionButton
        android:layout_width="match_parent"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@drawable/a1"
        android:id="@+id/materiA1"
        app:backgroundTint="@android:color/transparent"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="150dp"
        android:layout_height="65dp" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="60dp"
            android:gravity="center">

            <android.support.design.widget.FloatingActionButton
                android:layout_width="match_parent"
                android:clickable="true"
                app:fabSize="mini"
                app:srcCompat="@drawable/a5"
                android:id="@+id/materiA5"
                android:layout_weight="1"
                app:backgroundTint="@android:color/transparent"
                android:layout_gravity="center_horizontal"
                android:layout_marginRight="75dp"
                android:layout_height="65dp" />

            <android.support.design.widget.FloatingActionButton
                android:layout_width="match_parent"
                android:clickable="true"
                app:fabSize="mini"
                app:srcCompat="@drawable/q1"
                android:id="@+id/quiz1"
                app:backgroundTint="@android:color/transparent"
                android:layout_height="65dp"
                android:layout_weight="1"
                android:layout_gravity="center_horizontal" />

            <android.support.design.widget.FloatingActionButton
                android:layout_width="match_parent"
                android:clickable="true"
                app:fabSize="mini"
                app:srcCompat="@drawable/a2"
                android:id="@+id/materiA2"
                android:layout_weight="1"
                app:backgroundTint="@android:color/transparent"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="75dp"
                android:layout_height="65dp" />
        </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="60dp"
            android:gravity="center">

            <android.support.design.widget.FloatingActionButton
                android:layout_width="match_parent"
                android:clickable="true"
                app:fabSize="mini"
                app:srcCompat="@drawable/a4"
                android:id="@+id/materiA4"
                app:backgroundTint="@android:color/transparent"
                android:layout_gravity="center_horizontal"
                android:layout_height="65dp"
                app:elevation="7dp" />

            <android.support.design.widget.FloatingActionButton
                android:layout_width="match_parent"
                android:clickable="true"
                app:fabSize="mini"
                app:srcCompat="@drawable/a3"
                android:id="@+id/materiA3"
                android:layout_weight="1"
                app:backgroundTint="@android:color/transparent"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="75dp"
                android:layout_height="65dp" />
        </LinearLayout>
        </LinearLayout>

        <android.support.design.widget.FloatingActionButton
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            app:fabSize="mini"
            app:srcCompat="@drawable/ic_home_white_24dp"
            android:id="@+id/home"
            app:backgroundTint="@color/colorlevel"
            android:layout_marginTop="100dp"
            android:layout_gravity="center"/>
    </LinearLayout>


</LinearLayout>